import java.util.*;
public class HelloWorld {
public static void main(String[] args) {
  String s="pyrriyyyaa";
  char find='y';
  int count=0;
  for(int j=0;j<s.length();j++)
  {
  if(s.charAt(j)==find)
  {
      count++;
  }
  }
    System.out.println(count);
    }
}

// TIME COMPLEXITY: O(N)
//SPACE COMPLEXITY: O(1)
